<config>
        <service xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <timestamps>
            <debug>
              <datetime>
                <msec/>
              </datetime>
            </debug>
            <log>
              <datetime>
                <msec/>
              </datetime>
            </log>
          </timestamps>
        </service>
        <hostname xmlns="http://tail-f.com/ned/cisco-ios-xr">iosxrv-1</hostname>
        <vrf xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <vrf-list>
            <name>Mgmt-intf</name>
            <address-family>
              <ipv4>
                <unicast>
                </unicast>
              </ipv4>
              <ipv6>
                <unicast>
                </unicast>
              </ipv6>
            </address-family>
          </vrf-list>
        </vrf>
        <domain xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <lookup>
            <disable/>
          </lookup>
          <name>virl.info</name>
        </domain>
        <vty-pool xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <default>
            <first-vty>0</first-vty>
            <last-vty>50</last-vty>
          </default>
        </vty-pool>
        <telnet xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <vrf>
            <name>Mgmt-intf</name>
            <telnet-server-list>
              <af>ipv4</af>
              <server/>
              <max-servers>10</max-servers>
            </telnet-server-list>
          </vrf>
          <vrf>
            <name>default</name>
            <telnet-server-list>
              <af>ipv4</af>
              <server/>
              <max-servers>10</max-servers>
            </telnet-server-list>
          </vrf>
        </telnet>
        <control-plane xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <management-plane>
            <inband>
              <interface>
                <name>Loopback0</name>
              </interface>
              <interface>
                <name>MgmtEth0/0/CPU0/0</name>
              </interface>
              <interface>
                <name>all</name>
              </interface>
            </inband>
          </management-plane>
        </control-plane>
        <ssh xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <server>
            <v2/>
            <vrf>
              <name>Mgmt-intf</name>
            </vrf>
          </server>
        </ssh>
        <line xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <console>
            <exec-timeout>
              <minutes>0</minutes>
              <seconds>0</seconds>
            </exec-timeout>
          </console>
          <default>
            <exec-timeout>
              <minutes>720</minutes>
              <seconds>0</seconds>
            </exec-timeout>
          </default>
          <template>
            <name>vty</name>
            <timestamp>
            </timestamp>
            <exec-timeout>
              <minutes>720</minutes>
              <seconds>0</seconds>
            </exec-timeout>
          </template>
        </line>
        <interface xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <preconfigure>
            <GigabitEthernet>
              <id>0/0/0/0</id>
              <ipv4>
                <address>
                  <ip>10.0.0.5</ip>
                  <mask>255.255.255.252</mask>
                </address>
              </ipv4>
            </GigabitEthernet>
          </preconfigure>
        </interface>
        <route-policy xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <name>bgp_in</name>
          <cmd>
            <value>pass</value>
          </cmd>
        </route-policy>
        <route-policy xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <name>bgp_out</name>
          <cmd>
            <value>pass</value>
          </cmd>
        </route-policy>
        <router xmlns="http://tail-f.com/ned/cisco-ios-xr">
          <ospf>
            <name>1</name>
            <log>
              <adjacency>
                <changes>
                </changes>
              </adjacency>
            </log>
            <area>
              <id>0</id>
              <interface>
                <name>GigabitEthernet0/0/0/0</name>
                <cost>1</cost>
              </interface>
              <interface>
                <name>Loopback0</name>
                <passive>
                  <mode>enable</mode>
                </passive>
              </interface>
            </area>
          </ospf>
          <bgp>
            <bgp-no-instance>
              <id>1</id>
              <bgp>
                <router-id>192.168.0.1</router-id>
              </bgp>
              <address-family>
                <ipv4>
                  <unicast>
                    <network>
                      <net>192.168.0.1/32</net>
                    </network>
                  </unicast>
                </ipv4>
              </address-family>
              <neighbor>
                <id>192.168.0.2</id>
                <remote-as>1</remote-as>
                <address-family>
                  <ipv4>
                    <unicast>
                    </unicast>
                  </ipv4>
                </address-family>
                <update-source>
                  <Loopback>0</Loopback>
                </update-source>
              </neighbor>
            </bgp-no-instance>
          </bgp>
        </router>
      </config>